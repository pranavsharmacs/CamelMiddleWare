<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:chksvc="urn:v1.checkservice.vss.objects.bgc" xmlns:req0="urn:v1.requestcheckcreditworthiness.bps.objects.bgc" xmlns:res0="urn:v1.responsecheckcreditworthiness.bps.objects.bgc" xmlns:tns="urn:v1.financialbackgroundchecking.bps.services.bgc" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:v1.financialbackgroundchecking.bps.services.bgc" version="1.0">
