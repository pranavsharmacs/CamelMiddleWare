<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Belgacom (Belgacom) --><!-- ==================================================== --><!-- VSSContext                                          --><!-- ==================================================== --><xsd:schema targetNamespace="urn:v4.context.vss.objects.bgc" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn:v4.context.vss.objects.bgc" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.0">	<!-- ==================================================== -->	<!-- Transfer objects used in this interface              -->	<!-- ==================================================== -->	<xsd:element name="Context">		<xsd:annotation>			<xsd:documentation>			   Virtual Service Space Context regrouping others sub-contexts 			</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="tns:correlationId"/>				<xsd:element ref="tns:consumerApplicationId"/>				<xsd:element ref="tns:endUserId" minOccurs="0"/>				<xsd:element ref="tns:endUserLanguage" minOccurs="0"/>				<xsd:element ref="tns:propagatedKey" minOccurs="0"/>			</xsd:sequence>			<xsd:attribute name="version" type="xsd:string" default="4.0"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="endUserId" type="xsd:string">		<xsd:annotation>			<xsd:documentation>				Identifies the end user that is originating the call.				This element is mandatory.			</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="endUserLanguage" default="EN">		<xsd:annotation>			<xsd:documentation>				Identifies the end user language that is originating the call				This element is not mandatory. (the default is EN)			</xsd:documentation>		</xsd:annotation>		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:enumeration value="EN"/>				<xsd:enumeration value="FR"/>				<xsd:enumeration value="NL"/>				<xsd:enumeration value="DE"/>			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="consumerApplicationId" type="xsd:string">		<xsd:annotation>			<xsd:documentation>				Identifies the application that is originating the call.				Come from the list of application acronyms like ABD, ABD/FH, ...				This element is mandatory			</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="correlationId" type="xsd:string">		<xsd:annotation>			<xsd:documentation>				Information used by the service consumer to identify the request.				It can be interesting to have a unique value per request 				This element is mandatory.		   </xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="propagatedKey" type="xsd:string">		<xsd:annotation>			<xsd:documentation>				Key containing the logging profile to propagate from hops to hops				This element is optional and will be provided by the VSS logging system.			</xsd:documentation>		</xsd:annotation>	</xsd:element></xsd:schema>